{
  "ecmaFeatures": {
    "blockBindings": true,
    "forOf": true,
    "jsx": true
  },
  "globals": {
    "__DEV__": true
  },
  "plugins": [
    "react",
    "react-native"
  ],
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "semi": 0,
    "react/jsx-quotes": 0,
    "jsx-quotes": [2, "prefer-double"],
    "no-use-before-define": [2, "nofunc"],
    "indent": [2, 2, {"SwitchCase": 1}],
    "react/jsx-boolean-value": 0,
    "id-length": [2, {"exceptions": ["x", "y", "Q", "e"]}]
  },
  "env": {
    "node": true,
    "mocha": true,
    "jasmine": true,
    "jquery": true
  }
}
